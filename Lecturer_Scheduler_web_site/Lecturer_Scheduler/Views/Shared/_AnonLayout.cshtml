<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/graphics/StyleLogin1.css")" rel="stylesheet" type="text/css" />
   
  <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title" align="center">
                <h1>
                <marquee behavior="scroll" direction="left">
                <p style="font-size: xx-large; font-weight: bolder; font-style: italic; color: #000000; font-family: Arial;">Lecturer Secheduler ברוכים הבאים</p>
                </marquee>
                </h1>

            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
          
        </header>
        <section id="main">
            <div>
                @RenderBody()
            </div>
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>





